-- CRIAR COMANDOS - TELEPORT
CreateCommandButton(TeleportPage, "Goto Player", "Ir para um jogador (digite nome no chat)", function()
    CreateNotification("Digite o nome do jogador no chat!")
    local chatConnection
    chatConnection = player.Chatted:Connect(function(msg)
        chatConnection:Disconnect()
        GotoPlayer(msg)
    end)
end, false)

CreateCommandButton(TeleportPage, "Set Waypoint", "Definir ponto de teleporte", function()
    local root = GetRoot()
    if root then
        waypoints["Custom"] = root.CFrame
        CreateNotification("Waypoint salvo!")
    end
end, false)

CreateCommandButton(TeleportPage, "TP to Waypoint", "Teleportar para waypoint", function()
    local root = GetRoot()
    if root and waypoints["Custom"] then
        root.CFrame = waypoints["Custom"]
        CreateNotification("Teleportado para waypoint!")
    else
        CreateNotification("Nenhum waypoint definido!")
    end
end, false)

CreateCommandButton(TeleportPage, "Spawn Point", "Definir ponto de spawn", function()
    local root = GetRoot()
    if root then
        waypoints["Spawn"] = root.CFrame
        
        player.CharacterAdded:Connect(function(char)
            task.wait(0.5)
            local newRoot = char:WaitForChild("HumanoidRootPart")
            if newRoot and waypoints["Spawn"] then
                newRoot.CFrame = waypoints["Spawn"]
            end
        end)
        
        CreateNotification("Spawn point definido!")
    end
end, false)

CreateCommandButton(TeleportPage, "Bring All Players", "Traz todos para voc√™ (CLIENT)", function()
    local root = GetRoot()
    if not root then return end
    
    for _, targetPlayer in pairs(Players:GetPlayers()) do
        if targetPlayer ~= player and targetPlayer.Character then
            local targetRoot = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
            if targetRoot then
                targetRoot.CFrame = root.CFrame
            end
        end
    end
    CreateNotification("Todos os jogadores trazidos!")
end, false)

CreateCommandButton(TeleportPage, "TP to Spawn", "Voltar ao spawn", function()
    local root = GetRoot()
    if root then
        for _, spawn in pairs(Workspace:GetDescendants()) do
            if spawn:IsA("SpawnLocation") then
                root.CFrame = spawn.CFrame + Vector3.new(0, 3, 0)
                CreateNotification("Teleportado para spawn!")
                return
            end
        end
        CreateNotification("Spawn n√£o encontrado!")
    end
end, false)

