    local hum = GetHumanoid()
    if hum then hum.WalkSpeed = 50 end
end, false)
CreateCommandButton(PlayerPage, "Speed 100", "Velocidade 100", function() 
    local hum = GetHumanoid()
    if hum then hum.WalkSpeed = 100 end
end, false)
