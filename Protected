    local root = GetRoot()
    if root then
        local spin = Instance.new("BodyAngularVelocity")
        spin.Name = "Spin"
        spin.MaxTorque = Vector3.new(0, math.huge, 0)
        spin.AngularVelocity = Vector3.new(0, 25, 0)
        spin.Parent = root
        
        task.wait(5)
        spin:Destroy()
    end
end, false)

CreateCommandButton(LocalPage, "Platform", "Criar plataforma", function()
    local root = GetRoot()
    if root then
        local platform = Instance.new("Part")
        platform.Name = "Platform"
        platform.Size = Vector3.new(10, 1, 10)
        platform.Anchored = true
        platform.Position = root.Position - Vector3.new(0, 3, 0)
        platform.BrickColor = BrickColor.new("Bright blue")
        platform.Material = Enum.Material.Neon
        platform.Parent = Workspace
        
        CreateNotification("Plataforma criada! (5s)")
        
        task.wait(5)
        platform:Destroy()
    end
end, false)

CreateCommandButton(LocalPage, "Highlight Self", "Destacar personagem", function()
    local char = GetCharacter()
    if char and not char:FindFirstChild("SelfHighlight") then
        local highlight = Instance.new("Highlight")
        highlight.Name = "SelfHighlight"
        highlight.Adornee = char
        highlight.FillColor = Color3.fromRGB(100, 200, 255)
        highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
        highlight.Parent = char
        CreateNotification("Highlight ativado!")
    else
        if char and char:FindFirstChild("SelfHighlight") then
            char.SelfHighlight:Destroy()
            CreateNotification("Highlight desativado!")
        end
    end
end, false)

