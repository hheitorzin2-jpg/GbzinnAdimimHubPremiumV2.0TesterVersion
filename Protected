-- Fun√ß√£o de Notifica√ß√£o
function CreateNotification(text)
    local NotifFrame = Instance.new("Frame")
    NotifFrame.Parent = ScreenGui
    NotifFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 45)
    NotifFrame.BorderSizePixel = 0
    NotifFrame.Position = UDim2.new(1, -320, 1, 0)
    NotifFrame.Size = UDim2.new(0, 300, 0, 70)
    
    local NotifCorner = Instance.new("UICorner")
    NotifCorner.CornerRadius = UDim.new(0, 10)
    NotifCorner.Parent = NotifFrame
    
    local NotifIcon = Instance.new("TextLabel")
    NotifIcon.Parent = NotifFrame
    NotifIcon.BackgroundTransparency = 1
    NotifIcon.Position = UDim2.new(0, 10, 0, 0)
    NotifIcon.Size = UDim2.new(0, 40, 1, 0)
    NotifIcon.Font = Enum.Font.GothamBold
    NotifIcon.Text = "‚úì"
    NotifIcon.TextColor3 = Color3.fromRGB(100, 255, 100)
    NotifIcon.TextSize = 24
    
    local NotifText = Instance.new("TextLabel")
    NotifText.Parent = NotifFrame
    NotifText.BackgroundTransparency = 1
    NotifText.Size = UDim2.new(1, -60, 1, 0)
    NotifText.Position = UDim2.new(0, 50, 0, 0)
    NotifText.Font = Enum.Font.Gotham
    NotifText.Text = text
    NotifText.TextColor3 = Color3.fromRGB(255, 255, 255)
    NotifText.TextSize = 13
    NotifText.TextWrapped = true
    NotifText.TextXAlignment = Enum.TextXAlignment.Left
    
    NotifFrame:TweenPosition(UDim2.new(1, -320, 1, -90), "Out", "Quad", 0.5, true)
    
    task.wait(3)
    
    NotifFrame:TweenPosition(UDim2.new(1, -320, 1, 0), "In", "Quad", 0.5, true)
    task.wait(0.5)
    NotifFrame:Destroy()
end

-- Tecla de Atalho (Right Ctrl)
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and input.KeyCode == Enum.KeyCode.RightControl then
        MainFrame.Visible = not MainFrame.Visible
    end
end)

