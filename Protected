    character = char
    humanoid = char:WaitForChild("Humanoid")
    rootPart = char:WaitForChild("HumanoidRootPart")
    
    -- Reativar comandos persistentes
    if commandStates.antiAfk then
        ToggleAntiAFK()
        ToggleAntiAFK()
    end
end)

